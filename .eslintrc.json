{
	"extends": "airbnb",
	"plugins": ["jest"],
	"env": {
		"jest/globals": true
	},
	"rules": {
		"no-tabs": 0,
		"indent": ["error", "tab"],
		"quotes": [2, "double"],
		"react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx"] }],
		"react/jsx-indent": [2, "tab"],
		"import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
	}
}